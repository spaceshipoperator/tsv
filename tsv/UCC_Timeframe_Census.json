{
  "name": "UCC Timeframe Census",
  "fromDate":{
    "label": "from date: ",
    "isModifiable": true,
    "default": { "value": "2011-08-29" },
    "minimum": { "value": "2010-01-01" }
  },
  "series":{
    "label": "departments: ",
    "multiple" : true,
    "options": [
      [ 30401, "Auburn UCC" ],
      [ 30148, "Covington UCC" ],
      [ 70648, "Gig Harbor UCC" ],
      [ 30348, "Kent UCC" ],
      [ 70548, "Lakewood UCC" ],
      [ 70248, "Spanaway UCC" ],
      [ 70348, "University Place UCC" ],
      [ 70448, "Westgate UCC" ]
    ],
    "default": { "value": "30148,30348,30401,70248,70348,70448,70548,70648" }
  },
  "selectionScope" : {
    "label" : "specify date range",
    "multiple" : false,
    "options": [
      [ 1, "Selected Date Only" ],
      [ 2, "Work Week Of Selected Date" ],
      [ 3, "5 Weeks Of Selected Weekday" ]
    ],
    "default": { "value": "1" }
  },
  "xLabels": {
      "value": "6:40a,9:10a,11:40a,2:10p,4:40p,7:10p,9:40p"
  },
  "yLeft": { 
    "label" : "left vertical axis",
    "multiple" : true,
    "options":[
      [ 1, "actual wait time for clinical access" ],
      [ 2, "predicted wait time for clinical access" ]
    ],
    "default": { "value": "1" }
  },
  "yRight": { 
    "label" : "right vertical axis",
    "multiple" : true,
    "options":[
      [ 1, "number of patients waiting for clinical access" ],
      [ 2, "number of patients seen" ]
    ],
    "default": { "value": "1" }
  },
  "cmd": "exec edw.UCC.TimeframeCensusTS ~fromDate, ~series, ~selectionScope;"
}

{
  "name": "UCC Timeframe Census",
  "fromDate":{
    "label": "from date: ",
    "isModifiable": true,
    "default": { "value": "2011-08-29" },
    "minimum": { "value": "2010-01-01" }
  },
  "untilDate":{
    "isModifiable": false,
    "default": { "function": "daysAfter(~fromDate, 1)" }
  },
  "series":{
    "label": "departments: ",
    "options": [
      [ 30401, "Auburn UCC" ],
      [ 30148, "Covington UCC" ],
      [ 70648, "Gig Harbor UCC" ],
      [ 30348, "Kent UCC" ],
      [ 70548, "Lakewood UCC" ],
      [ 70248, "Spanaway UCC" ],
      [ 70348, "University Place UCC" ],
      [ 70448, "Westgate UCC" ]
    ],
    "default": { "value": "30401,30148,70648,30348,70548,70248,70348,70448" }
  },
  "yLeft": { 
    "options":[
      [1, "wait time for clinical access"],
      [2, "wait time for vitals taken" ]
    ],
    "default": { "value": "1" }
  },
  "yRight": { 
    "options":[
      [1, "number of patients waiting for clinical access"],
      [2, "number of patients waiting for vitals to be taken"],
      [3, "number of patients seen" ]
    ],
    "default": { "value": "1" }
  },
  "cmd": "exec edw.UCC.TimeframeCensusTS ~fromDate, ~untilDate, ~series;"
}
